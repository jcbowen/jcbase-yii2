{
  "name": "jcbowen/jcbase-yii2",
  "description": "自用yii2基础扩展库，提供一些常用的封装，方便开发。",
  "keywords": [
    "yii2",
    "jcbowen",
    "jcbase",
    "jiuchet"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Jcbowen\\JcbaseYii2\\": "src/",
      "Jcbowen\\JcbaseYii2\\console\\": "console/"
    }
  },
  "authors": [
    {
      "name": "jcbowen",
      "email": "bowen@jiuchet.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2.0 <8.0.0",
    "yiisoft/yii2": "^2.0.11",
    "yiisoft/yii2-redis": "^2.0.6",
    "phpoffice/phpspreadsheet": "^1.12",
    "aliyuncs/oss-sdk-php": "~2.4",
    "qcloud/cos-sdk-v5": "^2.5",
    "wechatpay/wechatpay": "^1.4"
  },
  "require-dev": {
    "ext-openssl": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-gd": "*",
    "ext-simplexml": "*",
    "ext-bcmath": "*",
    "ext-libxml": "*",
    "ext-pdo": "*",
    "ext-iconv": "*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
